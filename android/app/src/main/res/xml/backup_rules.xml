<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Exclude sensitive biometric data from backups -->
    <exclude domain="sharedpref" path="flutter_secure_storage.xml"/>
    <exclude domain="database" path="secure_storage.db"/>
    <exclude domain="file" path="face_data"/>
    <exclude domain="file" path="app_flutter/face_data"/>
    <exclude domain="file" path=".flutter_secure_storage"/>
</full-backup-content>